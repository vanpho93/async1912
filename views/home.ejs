<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Hello Async</title>
    <script type="text/javascript">

      let aPromise = new Promise((resolve, reject) => {
        setTimeout(() => {
          reject(10);
        }, 3000);
      });

      console.log(aPromise);

      setTimeout(() => {
        console.log(aPromise);
      }, 3500);

    </script>
  </head>
  <body>
    <h1>KhoaPham Training</h1>
  </body>
</html>
